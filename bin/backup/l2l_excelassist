# configuration vars
DOMAIN_NAME="excelassist.com"
DOMAIN_USER="excelass"
REMOTE_DIR_WWW="/home3/${DOMAIN_USER}/public_html"

# load live2local function helpers
source ~/.skel/scripts/live2local.sh

l2l_site_ilance
l2l_do_sync ${@}